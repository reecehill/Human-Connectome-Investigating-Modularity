{
  "include": [
    "scripts/*",
    "scripts/**/*"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "scripts/src/experimental",
    "scripts/src/typestubs",
    "scripts/src/**/*",
    "scripts/matlab",
    "matlab",
    "matlab/**/*",
    "scripts/matlab/**/*",
    "scripts/matlab",
    "logs/**/*"
  ],
  "ignore": [],
  "defineConstant": {
    "DEBUG": true
  },
  "stubPath": "typings/stubs",
  "venvPath": ".",
  "venv": ".venv/",
  "reportMissingImports": true,
  "reportMissingTypeStubs": true,
  "reportUnknownParameterType": true,
  "reportUntypedFunctionDecorator": true,
  "pythonVersion": "3.11",
  "pythonPlatform": "Linux",
  "executionEnvironments": [
    {
      "root": "scripts",
      "pythonVersion": "3.11",
      "pythonPlatform": "Linux",
      "extraPaths": []
    }
  ]
}