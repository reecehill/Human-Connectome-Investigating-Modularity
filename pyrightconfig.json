{
  "include": ["scripts/modules/**/*"],

  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "scripts/src/experimental",
    "scripts/src/typestubs",
    "scripts/src/**/*"
  ],

  "ignore": [],

  "defineConstant": {
    "DEBUG": true
  },

  "stubPath": "typings/stubs",
  "venv": "env367",

  "reportMissingImports": true,
  "reportMissingTypeStubs": true,

  "pythonVersion": "3.8",
  "pythonPlatform": "Linux",

  "executionEnvironments": [
    {
      "root": "src/web",
      "pythonVersion": "3.8",
      "pythonPlatform": "Windows",
      "extraPaths": ["src/service_libs"]
    },
    {
      "root": "src/sdk",
      "pythonVersion": "3.8",
      "extraPaths": ["src/backend"]
    },
    {
      "root": "src/tests",
      "extraPaths": ["src/tests/e2e", "src/sdk"]
    },
    {
      "root": "src"
    }
  ]
}
